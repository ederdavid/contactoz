<h1>Contactos</h1>


<%= render :partial  => '/shared/account' %>



<table>
  <tr>
    <th>Title</th>
    <th>Name</th>
    <th>City</th>
    <th>State</th>
    <th>Country</th>
    <th>Date updated</th>
  </tr>

<% if @contacts %>
<% for contact in @contacts %>
	<tr>
	   <td><%=h contact.title %></td> 
        <td><%= link_to contact.name, contact_path(contact) %><td>
	<td><%=h contact.city %></td>
    	<td><%=h contact.state %></td>
    	<td><%=h contact.country %></td>
    	<td><%=h contact.date_updated %></td>
        </tr>
<% end %>
<% end %>

</table>

<% if @contacts %>
<%= will_paginate @contacts %>
<% end %>

<br />


