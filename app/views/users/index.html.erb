<h1>Listing users</h1>

<table>
  <tr>
    <th>Screen name</th>
    <th>First name</th>
    <th>Last name</th>
    <th>Profession</th>
    <th>Email</th>
    <th>Password</th>
    <th>Activity</th>
    <th>Level</th>
    <th>Rating</th>
    <th>Points</th>
    <th>Date created</th>
    <th>Data contribution</th>
  </tr>

<% @users.each do |user| %>
  <tr>
    <td><%=h user.screen_name %></td>
    <td><%=h user.first_name %></td>
    <td><%=h user.last_name %></td>
    <td><%=h user.profession %></td>
    <td><%=h user.email %></td>
    <td><%=h user.password %></td>
    <td><%=h user.activity_id %></td>
    <td><%=h user.level %></td>
    <td><%=h user.rating %></td>
    <td><%=h user.points %></td>
    <td><%=h user.date_created %></td>
    <td><%=h user.data_contribution %></td>
    <td><%= link_to 'Show', user %></td>
    <td><%= link_to 'Edit', edit_user_path(user) %></td>
    <td><%= link_to 'Destroy', user, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New user', new_user_path %>