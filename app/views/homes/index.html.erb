<% @mobile %>
<% if params[:servicios] %>
<%= render :partial => "shared/mobile_service", :object => @mobile %>
<% end %>

<% if params[:productos] %>
<%= render :partial => "shared/mobile_product", :object => @mobile %>
<% end %>

<% if params[:productos].nil? && params[:servicios].nil?  %>
<%= render :partial => "shared/mobile_company", :object => @mobile %>
<% end %>


