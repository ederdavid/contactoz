<h2> Contactos </h2>

<% form_tag homes_path, :method => 'get' do %>
<p>
<%= text_field_tag :search, params[:search] %>
<%= submit_tag "Search" , :name => nil %>
</p>
<% end %>



<% if @contacts %>
<%form_tag contact_saveds_path, method => :post do%>
	<% for contact in @contacts %>
		<li>
		<%check_box_tag "contact_saved_id[]" contact_saved_id %>
		<%= link_to contact.name, contact_path(contact) %> 
		</li>
	<% end %>
<% end %>


<%= will_paginate @contacts %>
<% end %>

